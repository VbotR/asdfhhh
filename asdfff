import nest_asyncio
import aiogram
from aiogram import Bot, types
from aiogram.dispatcher import Dispatcher
from aiogram.utils import executor
from aiogram import Bot, Dispatcher, executor
from aiogram.types import CallbackQuery, Message,\
    InlineKeyboardButton, InlineKeyboardMarkup


nest_asyncio.apply()   
    
bot=Bot("5384533001:AAFkP7NUEEh0873_ks95kWuR-Noe8KSwC_0")
dp=Dispatcher(bot)

@dp.message_handler(commands="start")
async def cmd_language(message: types.Message):
    keyboard = types.ReplyKeyboardMarkup(resize_keyboard=True)
    buttons = ["RU üá∑üá∫", "ENG üá¨üáß"]
    keyboard.add(*buttons)
    await message.answer("Welcome, choose your language", reply_markup=keyboard)



@dp.message_handler(lambda message: message.text == "RU üá∑üá∫")
async def callback_start(message: Message):
    await bot.send_message(
        chat_id=message.chat.id,
        reply_markup=start_kb,
        text="–°–ø–∞—Å–∏–±–æ, —á—Ç–æ –≤—ã–±—Ä–∞–ª–∏ –Ω–∞—Å ‚ù§")

start_kb=InlineKeyboardMarkup().row(
    InlineKeyboardButton(text="–ü—Ä–∏–≤–∞—Ç–Ω—ã–µ —á–∏—Ç—ã",callback_data='cheats'))
start_kb.add(InlineKeyboardButton('–§–æ—Ä—É–º', url='https://phoenix-hack.com/'))
start_kb.add(InlineKeyboardButton('Discord', url='https://discord.gg/nh8zFdCPhv'))
start_kb.add(InlineKeyboardButton('–í–∫–æ–Ω—Ç–∞–∫—Ç–µ', url='https://vk.com/satanosoft'))
start_kb.add(InlineKeyboardButton('–¢–µ–ª–µ–≥—Ä–∞–º', url='https://t.me/phoenix_satano'))



@dp.callback_query_handler(lambda c: c.data == 'cheats')
async def process_callback_cheats(callback_query: types.CallbackQuery):
    await bot.send_message(callback_query.from_user.id, '–ù–∞—à–∏ —Ç–æ–≤–∞—Ä—ã', reply_markup=cheats_kb)

cheats_kb=InlineKeyboardMarkup().row(
    InlineKeyboardButton(text="CSGO",callback_data='CSGO'))
cheats_kb.add(InlineKeyboardButton('PUBG', callback_data='PUBG'))
cheats_kb.add(InlineKeyboardButton('RUST', callback_data='RUST'))
cheats_kb.add(InlineKeyboardButton('APEX', callback_data='APEX'))
cheats_kb.add(InlineKeyboardButton('R6:Siege', callback_data='R6:Siege'))
cheats_kb.add(InlineKeyboardButton('LOL', callback_data='LOL'))

@dp.callback_query_handler(lambda c: c.data == 'CSGO')
async def process_callback_CSGO(callback_query: types.CallbackQuery):
    await bot.send_message(callback_query.from_user.id, '–ù–∞—à–∏ —Ç–æ–≤–∞—Ä—ã - CSGO', reply_markup=CSGO_kb)

CSGO_kb=InlineKeyboardMarkup().row(
    InlineKeyboardButton(text="Phoenix Lite",callback_data='Lite'))
CSGO_kb.add(InlineKeyboardButton('Phoenix Pro', callback_data='Pro'))

